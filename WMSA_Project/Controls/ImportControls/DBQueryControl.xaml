<UserControl x:Class="WMSA_Project.Controls.ImportControls.DBQueryControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WMSA_Project.Controls.ImportControls"
             xmlns:uc="clr-namespace:WMSA_Project.Controls.AttributeControls"
             mc:Ignorable="d" d:DesignWidth="800" Height="300">
    <UserControl.Resources>
        <BooleanToVisibilityConverter x:Key="BoolToVis"/>
    </UserControl.Resources>
    <GroupBox Grid.Row="1" Header="Select The Attributes for This Script">
        <StackPanel Orientation="Vertical">
            <uc:ScriptAttributesControl x:Name="SAC_Test" Attribute="TestName" />
            <uc:ScriptAttributesControl x:Name="SAC_Build" Attribute="BuildName"/>
            <uc:ScriptAttributesControl x:Name="SAC_Script" Attribute="ScriptName" IsEnabled="False"/>
            <DatePicker x:Name="Dt_Pckr"/>
        </StackPanel>
    </GroupBox>
</UserControl>
