<UserControl x:Class="WMSA_Project.Controls.ScriptControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WMSA_Project.Controls"
             mc:Ignorable="d" 
             Background="LightGray">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Vertical" x:Name="Stack_Attributes">
            <TextBlock Background="SteelBlue" x:Name="TxtBlck_TestGroup" HorizontalAlignment="Stretch" Foreground="White" FontSize="14" >AdminGenTask | 19.3.0.0-211 | 10\30\2019</TextBlock>
        </StackPanel>
        <StackPanel Grid.Row="1" x:Name="Stack_Transactions"/>
    </Grid>
</UserControl>
