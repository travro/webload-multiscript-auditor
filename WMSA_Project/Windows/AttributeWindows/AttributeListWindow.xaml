<Window x:Class="WMSA_Project.Windows.AttributeWindows.AttributeListWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WMSA_Project.Windows.AttributeWindows"
        mc:Ignorable="d"
        Title="List of Script Attributes" Height="450" Width="300" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen">
    <Border BorderBrush="AliceBlue" BorderThickness="2">
        <Grid>
            <ListView x:Name="List_View" ItemsSource="{Binding SelectableList}" SelectionChanged="List_View_SelectionChanged"/>
        </Grid>
    </Border>
</Window>
